#ranking {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: white;
  /*.rank-tab-top-empty{
    height: 12px;
    background-color: #e5e5e5;
    position: fixed;
    z-index: 11;
    top: 0;
    left: 0;
    right: 0;
  }*/
  .rank-tab-contain{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 11;
    background-color: #e5e5e5;
    height: 178px;
  }
  .user-info {
    display: flex;
    justify-content: center;
    align-items: center;
    .level-comm {
      margin-right: 34px;
    }
    .swich-zh {
      flex: 1;
      justify-content: flex-end;
      font-size: 32px; /*!px*/
      display: flex;
      align-items: center;
      color: #007aff;
    }
    .use-nic {
      font-size: 30px; /*!px*/
    }
    .use-nic-desc {
      width: 340px;
      display: flex;
      align-items: center;
      .use-name {
        margin-left: 14px;
        font-size: 30px; /*!px*/
        color: black;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
        width: 290px;
      }
    }
    .first-img {
     width: 170px;
      height: 160px;
      max-width: 170px;
      z-index: 10;
    }
    .mui-ellipsis {
      font-size: 22px; /*!px*/
      margin-top: 10px;
    }
    .show-langp {
      color: #ff635f;
      margin-left: 10px;
      margin-right: 10px;
    }
    .live-rank-img {
      width: 110px;
    }
    .top-img-main {
      display: flex;
      align-items: center;
      justify-content: center;
      position: absolute;
      margin-left: 53px;
      left: 0;
      height: 160px;
      .top-tx-img-cmm{
        z-index: 4;
        border-radius: 50%;
        width: 124px;
        margin-top: -9px;
      }
    }
    .other-leve {
      width: 86px;
      border-radius: 50%;
      max-width: 140px;
      height: inherit;
      margin-right: 61px;
    }
    .other-leve-10 {
     margin-left: -21px;
    }
    .level-px {
      color: #1ea2ff;
      font-size: 38px; /*!px*/
      margin-right: 37px;
    }
    .use-v-img-comm {
      margin-top: 80px;
      margin-left: -32px;
      z-index: 10;
      width: 36px;
    }
    .v-1{
      margin-top: 48px;
      margin-left: -37px;
    }
    .v-3 {
      margin-top: 60px;
    }
    .list-v-img {
      position: absolute;
      left: 148px;
      z-index: 10;
      // margin-top: 29px;
      width: 25px;
      top: 83px;
    }
  }
  .guanzhu-main {
    -webkit-border-radius: 32px;
    -moz-border-radius: 32px;
    border-radius: 32px;
    font-family: Arial;
    color: #ffffff;
    font-size: 24px; /*!px*/
    background: #ff775d;
   // padding: 10px 10px 10px 10px;
    border: solid #ff775d 0px;
    text-decoration: none;
    width: 98px;
    height: 48px;
    text-align: center;
    line-height: 48px;
  }
  .done-follow {
    border: 1px solid #8f8f94; /*!no*/
    color: #8f8f94;
    background-color: white;
  }
  .icon-main {
    width: 85px;
    height: 36px;
    background-color: #55eeff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 7px;
    .ranking-iocon {
      width: 27px;
     // padding-left: 13px;
    }
    .icon-main-title {
      height: 36px;
      line-height: 36px;
      margin-left: 6px;
     // padding-right: 13px;
      display: flex;
      align-items: center;
      .icon-main-title-vel {
        color: white;
        font-size: 26px; /*!px*/
      }
    }
  }
  .mui-table-view:last-child {
    border-bottom: none;
  }
  #ranking-list{
    position: relative;
    top: 176px;
  }
  .rank-tab-contain {
    // margin-top: 12px;
    .rank-tab-type {
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 34px; /*!px*/
      text-align: center;
      height: 80px;
      margin-top: 12px;
      color: #aaaaaa;
      .rank-tab-type-comm {
        flex: 1;
        background-color: #f6f6f6;
        height: 80px;
        line-height: 80px;
      }
      .rank-active {
        background-color: white;
        color: #1da2ff;
       // border-bottom: 2px solid #1da2ff; /*!no*/
      }
    }
    .rank-tab-img-comm {
      width: 72px;
      height: 80px;
    }
  }
  .rank-day-main {
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    height: 86px;
   // border-bottom: 1px solid #ededed; /*!no*/
    background-color: white;
    color: #666666;
    .rank-day-comm {
      flex: 1;
      font-size: 30px; /*!px*/
      height: 86px;
      line-height: 86px;
    }
    .rank-day-active {
      color: #1da2ff;
      border-bottom: 2px solid #1da2ff; /*!no*/
    }
  }
  .list-main {
    border-bottom: 1px solid #ededed; /*!no*/
  }
  .level-ba {
    background-color: #55e1ff;
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: center;
    padding-left: 15px;
    padding-right: 15px;
    height: 35px;
    line-height: 35px;
    width: 91px;
    padding-top: 2px;
    .level-img {
      width: 26px;
      margin-right: 7px;
    }
    .level-dec {
      color: white;
     // margin-left: 10px;
      height: 35px;
      line-height: 35px;
      display: flex;
      align-items: center;
      font-size: 24px; /*!px*/
    }
  }
  .leve-11-20 {
    background-color: #29caff;
  }
  .leve-21-30 {
    background-color: #177dfd;
  }
  .leve-31-40 {
    background-color: #0b62ff;
  }
  .leve-41-50 {
    background-color: #8b68ff;
  }
  .leve-51-60 {
    background-color: #8235ff;
  }
  .leve-61-70 {
    background-color: #bd02d7;
  }
  .leve-71-80 {
    background-color: #db00d8;
  }
  .leve-81-90 {
    background-color: #ff2a66;
  }
  .leve-91-100 {
    background-color: #ff5f5f;
  }
  .leve-101-110 {
    background-color: #fdab1b;
  }
  .leve-111 {
    background-color: #ffc11a;
  }
  .tip-main {
    position: fixed;
    z-index: 100;
    right: 0;
    top: 34px;
    margin-right: 30px;
    .tip-img {
      width: 34px;
    }
  }
  .warning-bg.active {
    /* position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(74, 72, 72, 0.79);
    z-index: 12; */
    background-color: rgba(74, 72, 72, 0.79);
    top:0;
    left:0;
    height:100%;
    width:100%;
    z-index:12;
    position:fixed;
  }
  .waring-cont {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 19;
  }
  .warning-main.active {
    width: 666px;
    height: 497px;
    .waring-title {
      position: absolute;
      left: 0;
      right: 0;
      text-align: center;
      margin-top: 28px;
      font-size: 31px; /*!px*/
      color: white;
      font-weight: bold;
      // margin-left: 70px;
      text-indent: 8px;
    }
    .warning-cont {
      background: url("../images/tip-02.png") no-repeat;
      background-size: 666px 497px;
      width: 666px;
      display: flex;
      align-items: center;
      justify-content: center;
      height: 400px;
      border-bottom-left-radius: 25px;
      border-bottom-right-radius: 25px;
      .warning-desc{
        font-size: 30px; /*!px*/
        line-height: 51px;
       // text-align: center;
        margin-top: 50px;
       // text-indent: 65px;
        padding: 14px;
        padding-left: 19px;
      }
      .warning-time {
        font-size: 27px; /*!px*/
        margin-top: 5px;
      }
    }
  }
  .empty-main {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    transform: translateY(50%);
    -webkit-transform: translateY(50%);
    -moz-transform: translateY(50%);
    -o-transform: translateY(50%);
    margin-bottom: 50%;
    .empty-cont {
      display: flex;
      align-items: center;
      justify-content: center;
      .empty-img {
        width: 250px;
      }
    }
    .empty-desc {
      text-align: center;
      font-size: 26px; /*!px*/
      color: #757575;
      margin-top: 10px;
    }
  }
}
.mui-popup-backdrop {
  background: none;
}